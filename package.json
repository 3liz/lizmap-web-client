{
  "name": "lizmap-web-client",
  "version": "3.10.0",
  "description": "Lizmap Web Client",
  "license": "ISC",
  "author": "3Liz",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "workspaces": [
    "assets",
    "tests/js-units",
    "tests/end2end"
  ],
  "scripts": {
    "build": "rspack build --config assets/webpack.prod.js",
    "cy:open": "cypress open --project ./tests/end2end/",
    "cy:test": "cypress run --headed --project ./tests/end2end/",
    "js:test": "mocha --recursive ./tests/js-units/node",
    "jsdoc": "make version-doc && make js-doc",
    "pr:open": "playwright test --config tests/end2end/playwright.config.ts --ui tests/end2end/",
    "pretest-fix": "eslint -c eslint.config.mjs --fix",
    "pretest": "eslint -c eslint.config.mjs",
    "style-fix": "stylelint \"**/*.css\" --fix",
    "style-check": "stylelint \"**/*.css\"",
    "watch": "rspack --watch --config assets/webpack.dev.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.x",
    "@playwright/test": "^1.55.x",
    "@rspack/cli": "^1.4.x",
    "@rspack/core": "^1.5.x",
    "@stylistic/eslint-plugin-js": "^4.2.x",
    "@turf/line-split": "^7.2.x",
    "chai": "^6.0.x",
    "cypress": "<10.0.0",
    "cypress-file-upload": "^5.0.x",
    "dompurify": "^3.2.x",
    "datatables.net-bs5": "^2.2.2",
    "datatables.net-buttons-bs5": "^3.2.2",
    "datatables.net-datetime": "^1.5.5",
    "datatables.net-searchbuilder-bs5": "^1.8.2",
    "eslint-plugin-jsdoc": "^60.2.x",
    "flatgeobuf": "~4.2.x",
    "jsts": "^2.11.x",
    "lit-html": "^3.3.x",
    "mocha": "^11.7.x",
    "ol": "^10.6.x",
    "ol-ext": "^4.0.x",
    "ol-wfs-capabilities": "^2.0.x",
    "playwright-ctrf-json-reporter": "^0.0.x",
    "postcss-lit": "^1.3.x",
    "proj4": "^2.19.x",
    "shpjs": "^6.1.x",
    "stylelint": "^16.25.x",
    "stylelint-config-standard": "^39.0.x",
    "svg-sprite-loader": "^6.0.x",
    "svgo": "^4.0.x",
    "svgo-loader": "^4.0.x",
    "undici": "^7.16.x",
    "webpack": "^5.102.x",
    "webpack-merge": "^6.0.x"
  }
}
