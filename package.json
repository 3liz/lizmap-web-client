{
  "name": "lizmap-web-client",
  "version": "3.10.0",
  "description": "Lizmap Web Client",
  "license": "ISC",
  "author": "3Liz",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "workspaces": [
    "assets",
    "tests/js-units",
    "tests/end2end"
  ],
  "scripts": {
    "build": "rspack build --config assets/webpack.prod.js",
    "cy:open": "cypress open --project ./tests/end2end/",
    "cy:test": "cypress run --headed --project ./tests/end2end/",
    "js:test": "mocha --recursive ./tests/js-units/node",
    "jsdoc": "make version-doc && make js-doc",
    "pr:open": "playwright test --tsconfig tests/end2end/playwright.config.ts --ui tests/end2end/",
    "pretest-fix": "eslint -c eslint.config.mjs --fix",
    "pretest": "eslint -c eslint.config.mjs",
    "style-fix": "stylelint \"**/*.css\" --fix",
    "style-check": "stylelint \"**/*.css\"",
    "watch": "rspack --watch --config assets/webpack.dev.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.x",
    "@playwright/test": "^1.53.x",
    "@rspack/cli": "^1.4.1",
    "@rspack/core": "^1.4.1",
    "@stylistic/eslint-plugin-js": "^4.2.x",
    "@turf/line-split": "^7.2.0",
    "chai": "^5.2.x",
    "cypress": "<10.0.0",
    "cypress-file-upload": "^5.0.8",
    "dompurify": "^3.2.6",
    "eslint-plugin-jsdoc": "^50.6.x",
    "flatgeobuf": "^4.0.x",
    "jsts": "^2.11.0",
    "lit-html": "^3.3.0",
    "mocha": "^11.7.X",
    "ol": "^10.6.1",
    "ol-ext": "^4.0.x",
    "ol-wfs-capabilities": "^2.0.0",
    "playwright-ctrf-json-reporter": "^0.0.x",
    "postcss-lit": "^1.2.0",
    "proj4": "^2.19.X",
    "shpjs": "^6.1.0",
    "stylelint": "^16.15.x",
    "stylelint-config-standard": "^38.0.x",
    "svg-sprite-loader": "^6.0.11",
    "svgo": "^3.2.0",
    "svgo-loader": "^4.0.0",
    "undici": "^6.21.x",
    "webpack-merge": "^6.0.1"
  }
}
