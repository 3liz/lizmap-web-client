{
  "name": "lizmap-web-client",
  "version": "3.8.8-pre",
  "description": "Lizmap Web Client",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "devDependencies": {
    "@playwright/test": "~1.54.1",
    "@rspack/cli": "~1.4.11",
    "@rspack/core": "~1.4.11",
    "@turf/line-split": "~7.2.0",
    "chai": "~5.2.x",
    "cypress": "<10.0.0",
    "cypress-file-upload": "~5.0.8",
    "dompurify": "~3.2.x",
    "eslint": "~9.32.x",
    "eslint-plugin-jsdoc": "~48.11.x",
    "flatgeobuf": "~3.38.x",
    "jsts": "~2.12.x",
    "lit-html": "~3.3.x",
    "mocha": "~10.8.x",
    "ol": "~10.6.x",
    "ol-ext": "~4.0.x",
    "ol-wfs-capabilities": "~2.0.x",
    "playwright-ctrf-json-reporter": "~0.0.x",
    "proj4": "~2.19.x",
    "shpjs": "~6.1.x",
    "svg-sprite-loader": "~6.0.x",
    "svgo": "~3.3.x",
    "svgo-loader": "~4.0.x",
    "undici": "~6.21.x",
    "webpack": "~5.101.x",
    "webpack-merge": "~6.0.x"
  },
  "scripts": {
    "pretest": "eslint assets/src",
    "pretest-fix": "eslint --fix assets/src",
    "jsdoc": "make version-doc && make js-doc",
    "build": "rspack build --config assets/webpack.prod.js",
    "watch": "rspack --watch --config assets/webpack.dev.js",
    "js:test": "mocha --recursive ./tests/js-units/node",
    "pr:open": "playwright test --ui tests/end2end/",
    "cy:open": "cypress open --project ./tests/end2end/",
    "cy:test": "cypress run --headed --project ./tests/end2end/"
  },
  "author": "3Liz",
  "license": "ISC",
  "workspaces": [
    "assets",
    "tests/js-units",
    "tests/end2end"
  ]
}
