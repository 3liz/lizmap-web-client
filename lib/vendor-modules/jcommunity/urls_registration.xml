<?xml version="1.0" encoding="utf-8"?>
<suburls xmlns="http://jelix.org/ns/suburls/1.0">
    <url pathinfo="/"                    action="registration:index" optionalTrailingSlash="true"/>
    <url pathinfo="/save"               action="registration:save"/>
    <url pathinfo="/saved"               action="registration:saved"/>
    <url pathinfo="/confirm"            action="registration:confirm"/>

    <url pathinfo="/password-reset/"         action="password_reset:index" />
