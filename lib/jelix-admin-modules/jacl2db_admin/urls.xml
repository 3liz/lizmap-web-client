<?xml version="1.0" encoding="UTF-8"?>
<suburls xmlns="http://jelix.org/ns/suburls/1.0">
    <url pathinfo="/groups"                         action="groups:index"/>
    <url pathinfo="/groups/rights"                  action="groups:rights"/>
    <url pathinfo="/groups/saverights"              action="groups:saverights"/>
    <url pathinfo="/groups/resource-rights"         action="groups:rightres"/>
    <url pathinfo="/groups/save-resource-rights"    action="groups:saverightres"/>
    <url pathinfo="/groups/new"                     action="groups:newgroup"/>
    <url pathinfo="/groups/rename"                  action="groups:changename"/>
    <url pathinfo="/groups/delete"                  action="groups:delgroup"/>
    <url pathinfo="/groups/setdefault"              action="groups:setdefault"/>
    <url pathinfo="/users"                          action="users:index"/>
    <url pathinfo="/users/:user"                    action="users:rights"
        actionoverride="users:removegroup,users:addgroup,users:saverights,users:rightres,users:saverightres"/>
</suburls>
